{
  "name": "Laravel",
  "description": "Laravel template",
  "versions": [
    {
      "name": "11",
      "templates": [
        {
          "path": "templates/laravel/11/laravel",
          "name": "Laravel 11",
          "lang": "php"
        }
      ]
    }
  ],
  "packageManagers": [
    {
      "name": "composer",
      "commands": [
        {
          "name": "install",
          "command": "composer install"
        },
        {
          "name": "migrate",
          "command": "php artisan migrate"
        },
        {
          "name": "key:generate",
          "command": "php artisan key:generate"
        }
      ]
    }
  ]
}
